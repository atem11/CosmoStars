&lt;h2&gt;&lt;span id=&quot;.D0.A1.D0.BC._.D1.82.D0.B0.D0.BA.D0.B6.D0.B5&quot;&gt;&lt;/span&gt;&lt;span class=&quot;mw-headline&quot; id=&quot;См._также&quot;&gt;См. также&lt;/span&gt;&lt;span class=&quot;mw-editsection&quot;&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;[&lt;/span&gt;&lt;a href=&quot;/w/index.php?title=%D0%92%D1%8B%D0%B1%D0%BE%D1%80%D1%8B_%D0%B2_%D0%92%D0%B5%D1%80%D1%85%D0%BE%D0%B2%D0%BD%D1%83%D1%8E_%D1%80%D0%B0%D0%B4%D1%83_%D0%A3%D0%BA%D1%80%D0%B0%D0%B8%D0%BD%D1%8B_2019:_%D0%BE%D0%BD%D0%BB%D0%B0%D0%B9%D0%BD-%D1%82%D1%80%D0%B0%D0%BD%D1%81%D0%BB%D1%8F%D1%86%D0%B8%D1%8F&amp;amp;action=edit&amp;amp;section=1&quot; title=&quot;Редактировать раздел «См. также»&quot;&gt;править&lt;/a&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;]&lt;/span&gt;&lt;/span&gt;&lt;/h2&gt;
