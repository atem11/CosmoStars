&lt;h2&gt;&lt;span id=&quot;.D0.A1.D0.BC._.D1.82.D0.B0.D0.BA.D0.B6.D0.B5&quot;&gt;&lt;/span&gt;&lt;span class=&quot;mw-headline&quot; id=&quot;См._также&quot;&gt;См. также&lt;/span&gt;&lt;span class=&quot;mw-editsection&quot;&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;[&lt;/span&gt;&lt;a href=&quot;/w/index.php?title=%D0%9E%D0%BD%D0%BB%D0%B0%D0%B9%D0%BD-%D1%82%D1%80%D0%B0%D0%BD%D1%81%D0%BB%D1%8F%D1%86%D0%B8%D1%8F_%D0%B2%D1%82%D0%BE%D1%80%D0%BE%D0%B3%D0%BE_%D1%82%D1%83%D1%80%D0%B0_%D0%9F%D1%80%D0%B5%D0%B7%D0%B8%D0%B4%D0%B5%D0%BD%D1%82%D1%81%D0%BA%D0%B8%D1%85_%D0%B2%D1%8B%D0%B1%D0%BE%D1%80%D0%BE%D0%B2_%D0%B2_%D0%A3%D0%BA%D1%80%D0%B0%D0%B8%D0%BD%D0%B5_2019&amp;amp;action=edit&amp;amp;section=1&quot; title=&quot;Редактировать раздел «См. также»&quot;&gt;править&lt;/a&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;]&lt;/span&gt;&lt;/span&gt;&lt;/h2&gt;
&lt;ul&gt;&lt;li&gt;Фотографии на Викискладе: &lt;a href=&quot;https://commons.wikimedia.org/wiki/Category:Ukrainian_presidential_election,_2019&quot; class=&quot;extiw&quot; title=&quot;c:Category:Ukrainian presidential election, 2019&quot;&gt;Ukrainian presidential election, 2019&lt;/a&gt;.&lt;/li&gt;&lt;/ul&gt;
