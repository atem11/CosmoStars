&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%A3%D0%BC%D0%B5%D1%80%D1%88%D0%B8%D0%B5_11_%D0%B8%D1%8E%D0%BB%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Умершие 11 июля&quot;&gt;Умершие 11 июля&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D0%BA%D0%B8_11_%D0%B8%D1%8E%D0%BB%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Праздники 11 июля&quot;&gt;Праздники 11 июля&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;&lt;/ul&gt;
