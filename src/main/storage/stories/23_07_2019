&lt;div class=&quot;center&quot;&gt;&lt;div class=&quot;thumb tnone&quot;&gt;&lt;div class=&quot;thumbinner&quot; style=&quot;width:502px;&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:%D0%A2%D1%83-95%D0%9C%D0%A1_%D0%B2_%D0%A0%D1%8F%D0%B7%D0%B0%D0%BD%D1%81%D0%BA%D0%BE%D0%B9_%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D0%B8.jpg&quot; class=&quot;image&quot;&gt;&lt;img alt=&quot;&quot; src=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/f/f9/%D0%A2%D1%83-95%D0%9C%D0%A1_%D0%B2_%D0%A0%D1%8F%D0%B7%D0%B0%D0%BD%D1%81%D0%BA%D0%BE%D0%B9_%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D0%B8.jpg/500px-%D0%A2%D1%83-95%D0%9C%D0%A1_%D0%B2_%D0%A0%D1%8F%D0%B7%D0%B0%D0%BD%D1%81%D0%BA%D0%BE%D0%B9_%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D0%B8.jpg&quot; decoding=&quot;async&quot; width=&quot;500&quot; height=&quot;333&quot; class=&quot;thumbimage&quot; srcset=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/f/f9/%D0%A2%D1%83-95%D0%9C%D0%A1_%D0%B2_%D0%A0%D1%8F%D0%B7%D0%B0%D0%BD%D1%81%D0%BA%D0%BE%D0%B9_%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D0%B8.jpg/750px-%D0%A2%D1%83-95%D0%9C%D0%A1_%D0%B2_%D0%A0%D1%8F%D0%B7%D0%B0%D0%BD%D1%81%D0%BA%D0%BE%D0%B9_%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D0%B8.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/f/f9/%D0%A2%D1%83-95%D0%9C%D0%A1_%D0%B2_%D0%A0%D1%8F%D0%B7%D0%B0%D0%BD%D1%81%D0%BA%D0%BE%D0%B9_%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D0%B8.jpg/1000px-%D0%A2%D1%83-95%D0%9C%D0%A1_%D0%B2_%D0%A0%D1%8F%D0%B7%D0%B0%D0%BD%D1%81%D0%BA%D0%BE%D0%B9_%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D0%B8.jpg 2x&quot; data-file-width=&quot;1200&quot; data-file-height=&quot;800&quot; /&gt;&lt;/a&gt;  &lt;div class=&quot;thumbcaption&quot;&gt;&lt;div class=&quot;magnify&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:%D0%A2%D1%83-95%D0%9C%D0%A1_%D0%B2_%D0%A0%D1%8F%D0%B7%D0%B0%D0%BD%D1%81%D0%BA%D0%BE%D0%B9_%D0%BE%D0%B1%D0%BB%D0%B0%D1%81%D1%82%D0%B8.jpg&quot; class=&quot;internal&quot; title=&quot;Увеличить&quot;&gt;&lt;/a&gt;&lt;/div&gt;&lt;div class=&quot;center&quot; s