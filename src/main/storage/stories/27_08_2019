&lt;li&gt;17 августа 2019: &lt;a href=&quot;/wiki/%D0%9A%D0%B8%D1%82%D0%B0%D0%B9_%D0%B7%D0%B0%D0%BF%D1%83%D1%81%D1%82%D0%B8%D0%BB_%D0%BA%D0%BE%D0%BC%D0%BC%D0%B5%D1%80%D1%87%D0%B5%D1%81%D0%BA%D1%83%D1%8E_%D1%80%D0%B0%D0%BA%D0%B5%D1%82%D1%83_Smart_Dragon-1&quot; title=&quot;Китай запустил коммерческую ракету Smart Dragon-1&quot;&gt;Китай запустил коммерческую ракету Smart Dragon-1&lt;/a&gt;&lt;/li&gt;&lt;/ul&gt;
