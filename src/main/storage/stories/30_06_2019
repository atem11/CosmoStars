&lt;ul&gt;&lt;li&gt;Закончится &lt;a href=&quot;/wiki/%D0%9A%D1%83%D0%B1%D0%BE%D0%BA_%D0%B0%D1%84%D1%80%D0%B8%D0%BA%D0%B0%D0%BD%D1%81%D0%BA%D0%B8%D1%85_%D0%BD%D0%B0%D1%86%D0%B8%D0%B9_2019&quot; class=&quot;mw-redirect&quot; title=&quot;Кубок африканских наций 2019&quot;&gt;32&lt;/a&gt; &lt;a href=&quot;/wiki/%D0%9A%D1%83%D0%B1%D0%BE%D0%BA_%D0%B0%D1%84%D1%80%D0%B8%D0%BA%D0%B0%D0%BD%D1%81%D0%BA%D0%B8%D1%85_%D0%BD%D0%B0%D1%86%D0%B8%D0%B9&quot; class=&quot;mw-redirect&quot; title=&quot;Кубок африканских наций&quot;&gt;Кубок африканских наций&lt;/a&gt; в &lt;a href=&quot;/wiki/%D0%9A%D0%B0%D0%BC%D0%B5%D1%80%D1%83%D0%BD&quot; class=&quot;mw-redirect&quot; title=&quot;Камерун&quot;&gt;Камеруне&lt;/a&gt;.&lt;/li&gt;
