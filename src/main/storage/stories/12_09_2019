&lt;ul&gt;&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%9B%D0%B5%D0%BD%D1%82%D0%B0_%D0%BD%D0%BE%D0%B2%D0%BE%D1%81%D1%82%D0%B5%D0%B9_12_%D1%81%D0%B5%D0%BD%D1%82%D1%8F%D0%B1%D1%80%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0&quot; title=&quot;Лента новостей 12 сентября 2019 года&quot;&gt;Лента новостей 12 сентября 2019 года&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%A0%D0%BE%D0%B4%D0%B8%D0%B2%D1%88%D0%B8%D0%B5%D1%81%D1%8F_12_%D1%81%D0%B5%D0%BD%D1%82%D1%8F%D0%B1%D1%80%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Родившиеся 12 сентября&quot;&gt;Родившиеся 12 сентября&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
