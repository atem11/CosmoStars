			&lt;div class=&quot;thumb&quot; style=&quot;width: 674px;&quot;&gt;&lt;div style=&quot;margin:0px auto;&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D1%87%D0%BD%D0%B0%D1%8F_%D0%B3%D1%80%D1%83%D0%BF%D0%BF%D0%B0_%D0%BA%D0%B0%D0%B4%D0%B5%D1%82%D0%BE%D0%B2_%D0%BD%D0%B0_%D1%84%D0%BE%D0%BD%D0%B5_%D0%A8%D0%B8%D1%80%D0%BE%D0%BA%D0%BE%D1%80%D0%B5%D1%87%D0%B5%D0%BD%D1%81%D0%BA%D0%BE%D0%B3%D0%BE_%D0%BC%D0%B5%D0%BC%D0%BE%D1%80%D0%B8%D0%B0%D0%BB%D0%B0_%D0%95%D0%BA%D0%B0%D1%82%D0%B5%D1%80%D0%B8%D0%BD%D0%B1%D1%83%D1%80%D0%B3_7_%D0%BC%D0%B0%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0.jpg&quot; class=&quot;image&quot;&gt;&lt;img alt=&quot;&quot; src=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/5/57/%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D1%87%D0%BD%D0%B0%D1%8F_%D0%B3%D1%80%D1%83%D0%BF%D0%BF%D0%B0_%D0%BA%D0%B0%D0%B4%D0%B5%D1%82%D0%BE%D0%B2_%D0%BD%D0%B0_%D1%84%D0%BE%D0%BD%D0%B5_%D0%A8%D0%B8%D1%80%D0%BE%D0%BA%D0%BE%D1%80%D0%B5%D1%87%D0%B5%D0%BD%D1%81%D0%BA%D0%BE%D0%B3%D0%BE_%D0%BC%D0%B5%D0%BC%D0%BE%D1%80%D0%B8%D0%B0%D0%BB%D0%B0_%D0%95%D0%BA%D0%B0%D1%82%D0%B5%D1%80%D0%B8%D0%BD%D0%B1%D1%83%D1%80%D0%B3_7_%D0%BC%D0%B0%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0.jpg/1011px-thumbnail.jpg&quot; decoding=&quot;async&quot; width=&quot;674&quot; height=&quot;450&quot; srcset=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/5/57/%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D1%87%D0%BD%D0%B0%D1%8F_%D0%B3%D1%80%D1%83%D0%BF%D0%BF%D0%B0_%D0%BA%D0%B0%D0%B4%D0%B5%D1%82%D0%BE%D0%B2_%D0%BD%D0%B0_%D1%84%D0%BE%D0%BD%D0%B5_%D0%A8%D0%B8%D1%80%D0%BE%D0%BA%D0%BE%D1%80%D0%B5%D1%87%D0%B5%D0%BD%D1%81%D0%BA%D0%BE%D0%B3%D0%BE_%D0%BC%D0%B5%D0%BC%D0%BE%D1%80%D0%B8%D0%B0%D0%BB%D0%B0_%D0%95%D0%BA%D0%B0%D1%82%D0%B5%D1%80%D0%B8%D0%BD%D0%B1%D1%83%D1%80%D0%B3_7_%D0%BC%D0%B0%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0.jpg/1517px-thumbnail.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/5/57/%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D1%87%D0%BD%D0%B0%D1%8F_%D0%B3%D1%80%D1%83%D0%BF%D0%BF%D0%B0_%D0%BA%D0%B0%D0%B4%D0%B5%D1%82%D0%BE%D0%B2_%D0%BD%D0%B0_%D1%84%D0%BE%D0%BD%D0%B5_%D0%A8%D0%B8%D1%80%D0%BE%D0%BA%D0%BE%D1%80%D0%B5%D1%87%D0%B5%D0%BD%D1%81%D0%BA%D0%BE%D0%B3%D0%BE_%D0%BC%D0%B5%D0%BC%D0%BE%D1%80%D0%B8%D0%B0%D0%BB%D0%B0_%D0%95%D0%BA%D0%B0%D1%82%D0%B5%D1%80%D0%B8%D0%BD%D0%B1%D1%83%D1%80%D0%B3_7_%D0%BC%D0%B0%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0.jpg/2021px-thumbnail.jpg 2x&quot; data-file-width=&quot;4000&quot; data-file-height=&quot;2672&quot; /&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;
