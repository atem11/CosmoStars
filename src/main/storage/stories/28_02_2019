&lt;div class=&quot;thumb tleft&quot;&gt;&lt;div class=&quot;thumbinner&quot; style=&quot;width:302px;&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:%D0%A2%D0%B0%D1%82%D1%8C%D1%8F%D0%BD%D0%B0_%D0%9C%D0%B5%D1%80%D0%B7%D0%BB%D1%8F%D0%BA%D0%BE%D0%B2%D0%B0_%D0%B8_%D0%93%D0%B5%D0%BD%D0%BD%D0%B0%D0%B4%D0%B8%D0%B9_%D0%91%D1%83%D1%80%D0%B1%D1%83%D0%BB%D0%B8%D1%81_%D0%B2_%D0%95%D0%BB%D1%8C%D1%86%D0%B8%D0%BD_%D1%86%D0%B5%D0%BD%D1%82%D1%80%D0%B5_27_%D1%84%D0%B5%D0%B2%D1%80%D0%B0%D0%BB%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0.jpg&quot; class=&quot;image&quot;&gt;&lt;img alt=&quot;&quot; src=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/7/73/%D0%A2%D0%B0%D1%82%D1%8C%D1%8F%D0%BD%D0%B0_%D0%9C%D0%B5%D1%80%D0%B7%D0%BB%D1%8F%D0%BA%D0%BE%D0%B2%D0%B0_%D0%B8_%D0%93%D0%B5%D0%BD%D0%BD%D0%B0%D0%B4%D0%B8%D0%B9_%D0%91%D1%83%D1%80%D0%B1%D1%83%D0%BB%D0%B8%D1%81_%D0%B2_%D0%95%D0%BB%D1%8C%D1%86%D0%B8%D0%BD_%D1%86%D0%B5%D0%BD%D1%82%D1%80%D0%B5_27_%D1%84%D0%B5%D0%B2%D1%80%D0%B0%D0%BB%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0.jpg/300px-%D0%A2%D0%B0%D1%82%D1%8C%D1%8F%D0%BD%D0%B0_%D0%9C%D0%B5%D1%80%D0%B7%D0%BB%D1%8F%D0%BA%D0%BE%D0%B2%D0%B0_%D0%B8_%D0%93%D0%B5%D0%BD%D0%BD%D0%B0%D0%B4%D0%B8%D0%B9_%D0%91%D1%83%D1%80%D0%B1%D1%83%D0%BB%D0%B8%D1%81_%D0%B2_%D0%95%D0%BB%D1%8C%D1%86%D0%B8%D0%BD_%D1%86%D0%B5%D0%BD%D1%82%D1%80%D0%B5_27_%D1%84%D0%B5%D0%B2%D1%80%D0%B0%D0%BB%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0.jpg&quot; decoding=&quot;async&quot; width=&quot;300&quot; height=&quot;200&quot; class=&quot;thumbimage&quot; srcset=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/7/73/%D0%A2%D0%B0%D1%82%D1%8C%D1%8F%D0%BD%D0%B0_%D0%9C%D0%B5%D1%80%D0%B7%D0%BB%D1%8F%D0%BA%D0%BE%D0%B2%D0%B0_%D0%B8_%D0%93%D0%B5%D0%BD%D0%BD%D0%B0%D0%B4%D0%B8%D0%B9_%D0%91%D1%83%D1%80%D0%B1%D1%83%D0%BB%D0%B8%D1%81_%D0%B2_%D0%95%D0%BB%D1%8C%D1%86%D0%B8%D0%BD_%D1%86%D0%B5%D0%BD%D1%82%D1%80%D0%B5_27_%D1%84%D0%B5%D0%B2%D1%80%D0%B0%D0%BB%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0.jpg/450px-%D0%A2%D0%B0%D1%82%D1%8C%D1%8F%D0%BD%D0%B0_%D0%9C%D0%B5%