&lt;h2&gt;&lt;span id=&quot;.D0.98.D1.81.D1.82.D0.BE.D1.87.D0.BD.D0.B8.D0.BA.D0.B8&quot;&gt;&lt;/span&gt;&lt;span class=&quot;mw-headline&quot; id=&quot;Источники&quot;&gt;Источники&lt;/span&gt;&lt;span class=&quot;mw-editsection&quot;&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;[&lt;/span&gt;&lt;a href=&quot;/w/index.php?title=%D0%92_%D0%9C%D0%BE%D1%81%D0%BA%D0%B2%D0%B5_%D0%B2_%D1%88%D1%82%D0%B0%D0%B1_%D0%9B%D1%8E%D0%B1%D0%BE%D0%B2%D0%B8_%D0%A1%D0%BE%D0%B1%D0%BE%D0%BB%D1%8C_%D0%B8_%D0%98%D0%B2%D0%B0%D0%BD%D0%B0_%D0%96%D0%B4%D0%B0%D0%BD%D0%BE%D0%B2%D0%B0_%D0%BF%D1%80%D0%B8%D1%88%D1%91%D0%BB_%D0%A1%D0%9A&amp;amp;action=edit&amp;amp;section=1&quot; title=&quot;Редактировать раздел «Источники»&quot;&gt;править&lt;/a&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;]&lt;/span&gt;&lt;/span&gt;&lt;/h2&gt;
&lt;table style=&quot;width:100%; text-align:center; border:1px solid #a2a9b1&quot; class=&quot;plainlinks&quot;&gt;

