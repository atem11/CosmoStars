&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%A3%D0%BC%D0%B5%D1%80%D1%88%D0%B8%D0%B5_29_%D0%BC%D0%B0%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Умершие 29 мая&quot;&gt;Умершие 29 мая&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D0%BA%D0%B8_29_%D0%BC%D0%B0%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Праздники 29 мая&quot;&gt;Праздники 29 мая&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;&lt;/ul&gt;
