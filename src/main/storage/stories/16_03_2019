&lt;h2&gt;&lt;span id=&quot;.D0.A1.D0.BB.D1.83.D1.88.D0.B0.D1.82.D1.8C&quot;&gt;&lt;/span&gt;&lt;span class=&quot;mw-headline&quot; id=&quot;Слушать&quot;&gt;Слушать&lt;/span&gt;&lt;span class=&quot;mw-editsection&quot;&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;[&lt;/span&gt;&lt;a href=&quot;/w/index.php?title=%D0%92%D0%BE%D1%81%D1%8C%D0%BC%D0%BE%D0%B9_%D0%B0%D0%BB%D1%8C%D0%B1%D0%BE%D0%BC_%D0%A1%D0%BC%D0%BE%D0%BA%D0%B8_%D0%9C%D0%BE_%C2%AB%D0%91%D0%B5%D0%BB%D1%8B%D0%B9_%D0%B1%D0%BB%D1%8E%D0%B7%C2%BB&amp;amp;action=edit&amp;amp;section=1&quot; title=&quot;Редактировать раздел «Слушать»&quot;&gt;править&lt;/a&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;]&lt;/span&gt;&lt;/span&gt;&lt;/h2&gt;
