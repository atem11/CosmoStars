&lt;h2&gt;&lt;span id=&quot;.D0.98.D1.81.D1.82.D0.BE.D1.87.D0.BD.D0.B8.D0.BA.D0.B8&quot;&gt;&lt;/span&gt;&lt;span class=&quot;mw-headline&quot; id=&quot;Источники&quot;&gt;Источники&lt;/span&gt;&lt;span class=&quot;mw-editsection&quot;&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;[&lt;/span&gt;&lt;a href=&quot;/w/index.php?title=%D0%9B%D0%B5%D0%BD%D1%82%D0%B0_%D0%BD%D0%BE%D0%B2%D0%BE%D1%81%D1%82%D0%B5%D0%B9_16_%D0%BC%D0%B0%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0&amp;amp;action=edit&amp;amp;section=1&quot; title=&quot;Редактировать раздел «Источники»&quot;&gt;править&lt;/a&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;]&lt;/span&gt;&lt;/span&gt;&lt;/h2&gt;
&lt;table class=&quot;plainlinks&quot; style=&quot;margin: 0 0 1em 1em; clear:right; border: solid #aaa 1px; background: #f9f9f9; padding: 1ex; font-size: 90%; float:right; width:300px&quot; id=&quot;navigation-box&quot;&gt;
