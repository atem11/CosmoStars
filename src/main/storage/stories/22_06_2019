&lt;ul&gt;&lt;li&gt;&lt;a href=&quot;/wiki/%D0%9C%D0%B8%D0%BD%D1%82%D1%80%D0%B0%D0%BD%D1%81&quot; class=&quot;mw-redirect&quot; title=&quot;Минтранс&quot;&gt;Минтранс&lt;/a&gt; с &lt;a href=&quot;/wiki/8_%D0%B8%D1%8E%D0%BB%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0&quot; class=&quot;mw-redirect&quot; title=&quot;8 июля 2019 года&quot;&gt;8 июля&lt;/a&gt; &lt;a rel=&quot;nofollow&quot; class=&quot;external text&quot; href=&quot;https://mintrans.ru/press-center/news/9162&quot;&gt;запретит&lt;/a&gt; &lt;a href=&quot;/wiki/%D0%93%D1%80%D1%83%D0%B7%D0%B8%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Грузия&quot;&gt;грузинским&lt;/a&gt; авиакомпаниям полёты в &lt;a href=&quot;/wiki/%D0%A0%D0%BE%D1%81%D1%81%D0%B8%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Россия&quot;&gt;Россию&lt;/a&gt;.&lt;/li&gt;
&lt;li&gt;Телеканал «&lt;a href=&quot;/wiki/%D0%A0%D0%BE%D1%81%D1%81%D0%B8%D1%8F-24&quot; class=&quot;mw-redirect&quot; title=&quot;Россия-24&quot;&gt;Россия-24&lt;/a&gt;» &lt;a rel=&quot;nofollow&quot; class=&quot;external text&quot; href=&quot;https://www.vesti.ru/doc.html?id=3160730&quot;&gt;сообщил&lt;/a&gt; о нападении &lt;a href=&quot;/wiki/%D0%93%D1%80%D1%83%D0%B7%D0%B8%D0%BD%D1%81%D0%BA%D0%B8%D0%B9_%D0%BD%D0%B0%D1%86%D0%B8%D0%BE%D0%BD%D0%B0%D0%BB%D0%B8%D0%B7%D0%BC&quot; class=&quot;mw-redirect&quot; title=&quot;Грузинский национализм&quot;&gt;грузинских националистов&lt;/a&gt; на журналиста в &lt;a href=&quot;/wiki/%D0%A2%D0%B1%D0%B8%D0%BB%D0%B8%D1%81%D0%B8&quot; class=&quot;mw-redirect&quot; title=&quot;Тбилиси&quot;&gt;Тбилиси&lt;/a&gt;.&lt;/li&gt;
