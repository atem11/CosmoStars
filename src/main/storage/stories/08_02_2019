&lt;ul&gt;&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%9B%D0%B5%D0%BD%D1%82%D0%B0_%D0%BD%D0%BE%D0%B2%D0%BE%D1%81%D1%82%D0%B5%D0%B9_8_%D1%84%D0%B5%D0%B2%D1%80%D0%B0%D0%BB%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0&quot; title=&quot;Лента новостей 8 февраля 2019 года&quot;&gt;Лента новостей 8 февраля 2019 года&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%A0%D0%BE%D0%B4%D0%B8%D0%B2%D1%88%D0%B8%D0%B5%D1%81%D1%8F_8_%D1%84%D0%B5%D0%B2%D1%80%D0%B0%D0%BB%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Родившиеся 8 февраля&quot;&gt;Родившиеся 8 февраля&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%A3%D0%BC%D0%B5%D1%80%D1%88%D0%B8%D0%B5_8_%D1%84%D0%B5%D0%B2%D1%80%D0%B0%D0%BB%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Умершие 8 февраля&quot;&gt;Умершие 8 февраля&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D0%BA%D0%B8_8_%D1%84%D0%B5%D0%B2%D1%80%D0%B0%D0%BB%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Праздники 8 февраля&quot;&gt;Праздники 8 февраля&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;&lt;/ul&gt;
&lt;/td&gt;
&lt;td class=&quot;widthhack&quot;&gt;
