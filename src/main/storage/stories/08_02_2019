&lt;h2&gt;&lt;span id=&quot;.D0.A1.D0.BC._.D1.82.D0.B0.D0.BA.D0.B6.D0.B5&quot;&gt;&lt;/span&gt;&lt;span class=&quot;mw-headline&quot; id=&quot;См._также&quot;&gt;См. также&lt;/span&gt;&lt;span class=&quot;mw-editsection&quot;&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;[&lt;/span&gt;&lt;a href=&quot;/w/index.php?title=%D0%92_%C2%AB%D0%91%D0%BE%D1%8F%D1%80%D1%81%D0%BA%D0%B8%D1%85_%D0%BF%D0%B0%D0%BB%D0%B0%D1%82%D0%B0%D1%85%C2%BB_%D0%BF%D0%BE%D0%B4%D0%B0%D0%BB%D0%B8_%C2%AB%D0%9F%D1%83%D0%BB%D0%B8_%D0%B2_%D1%88%D0%BE%D0%BA%D0%BE%D0%BB%D0%B0%D0%B4%D0%B5%C2%BB&amp;amp;action=edit&amp;amp;section=1&quot; title=&quot;Редактировать раздел «См. также»&quot;&gt;править&lt;/a&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;]&lt;/span&gt;&lt;/span&gt;&lt;/h2&gt;
