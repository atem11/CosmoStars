&lt;/p&gt;
			&lt;/div&gt;
		&lt;/div&gt;&lt;/li&gt;
		&lt;li class=&quot;gallerybox&quot; style=&quot;width: 302px&quot;&gt;&lt;div style=&quot;width: 302px&quot;&gt;
			&lt;div class=&quot;thumb&quot; style=&quot;width: 300px;&quot;&gt;&lt;div style=&quot;margin:0px auto;&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:Golunov_march_009.jpg&quot; class=&quot;image&quot;&gt;&lt;img alt=&quot;&quot; src=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/9/9f/Golunov_march_009.jpg/450px-Golunov_march_009.jpg&quot; decoding=&quot;async&quot; width=&quot;300&quot; height=&quot;400&quot; srcset=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/9/9f/Golunov_march_009.jpg/675px-Golunov_march_009.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/9/9f/Golunov_march_009.jpg/900px-Golunov_march_009.jpg 2x&quot; data-file-width=&quot;3456&quot; data-file-height=&quot;4608&quot; /&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;
