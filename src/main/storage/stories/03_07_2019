&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%A0%D0%BE%D0%B4%D0%B8%D0%B2%D1%88%D0%B8%D0%B5%D1%81%D1%8F_3_%D0%B8%D1%8E%D0%BB%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Родившиеся 3 июля&quot;&gt;Родившиеся 3 июля&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%A3%D0%BC%D0%B5%D1%80%D1%88%D0%B8%D0%B5_3_%D0%B8%D1%8E%D0%BB%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Умершие 3 июля&quot;&gt;Умершие 3 июля&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D0%BA%D0%B8_3_%D0%B8%D1%8E%D0%BB%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Праздники 3 июля&quot;&gt;Праздники 3 июля&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;&lt;/ul&gt;
