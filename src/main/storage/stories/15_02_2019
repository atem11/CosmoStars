		&lt;li class=&quot;gallerybox&quot; style=&quot;width: 535.33333333333px&quot;&gt;&lt;div style=&quot;width: 535.33333333333px&quot;&gt;
			&lt;div class=&quot;thumb&quot; style=&quot;width: 533.33333333333px;&quot;&gt;&lt;div style=&quot;margin:0px auto;&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:Opening_Red_Factory_Co-working_(2019-01-25)_13.jpg&quot; class=&quot;image&quot;&gt;&lt;img alt=&quot;Opening Red Factory Co-working (2019-01-25) 13.jpg&quot; src=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/8/8b/Opening_Red_Factory_Co-working_%282019-01-25%29_13.jpg/800px-Opening_Red_Factory_Co-working_%282019-01-25%29_13.jpg&quot; decoding=&quot;async&quot; width=&quot;534&quot; height=&quot;400&quot; srcset=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/8/8b/Opening_Red_Factory_Co-working_%282019-01-25%29_13.jpg/1200px-Opening_Red_Factory_Co-working_%282019-01-25%29_13.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/8/8b/Opening_Red_Factory_Co-working_%282019-01-25%29_13.jpg/1600px-Opening_Red_Factory_Co-working_%282019-01-25%29_13.jpg 2x&quot; data-file-width=&quot;3648&quot; data-file-height=&quot;2736&quot; /&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;
		&lt;/div&gt;&lt;/li&gt;
		&lt;li class=&quot;gallerybox&quot; style=&quot;width: 535.33333333333px&quot;&gt;&lt;div style=&quot;width: 535.33333333333px&quot;&gt;
			&lt;div class=&quot;thumb&quot; style=&quot;width: 533.33333333333px;&quot;&gt;&lt;div style=&quot;margin:0px auto;&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:Opening_Red_Factory_Co-working_(2019-01-25)_14.jpg&quot; class=&quot;image&quot;&gt;&lt;img alt=&quot;Opening Red Factory Co-working (2019-01-25) 14.jpg&quot; src=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/b/ba/Opening_Red_Factory_Co-working_%282019-01-25%29_14.jpg/800px-Opening_Red_Factory_Co-working_%282019-01-25%29_14.jpg&quot; decoding=&quot;async&quot; width=&quot;534&quot; height=&quot;400&quot; srcset=&quot;//upload.wikimedia.org/wikipedia/commons/thumb/b/ba/Opening_Red_Factory_Co-working_%282019-01-25%29_14.jpg/1200px-Opening_Red_Factory_Co-working_%282019-01-25%29_14.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/b/ba/Opening_Red_Factory_Co-working_%282019-01-25%29_14.jpg/1600px-Opening_Red_Factory_Co-working_%282019-01-25%29_14.jpg 2x&quot; data-file-width=&quot;3648&quot; data-file-height=&quot;2736&quot; /&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;
		&lt;/div&gt;&lt;/li&gt;
