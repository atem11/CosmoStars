&lt;td&gt;«&lt;a href=&quot;/w/index.php?title=%D0%94%D0%BE%D1%80%D0%BE%D0%B3%D0%B8%D0%B5_%D0%BC%D0%BE%D0%B8_%D1%81%D1%82%D0%B0%D1%80%D0%B8%D0%BA%D0%B8_(%D0%BF%D0%B5%D1%81%D0%BD%D1%8F)&amp;amp;action=edit&amp;amp;redlink=1&quot; class=&quot;new&quot; title=&quot;Дорогие мои старики (песня) (страница не существует)&quot;&gt;Дорогие мои старики&lt;/a&gt;»&lt;sub&gt;(&lt;a href=&quot;/wiki/%D0%98%D0%B3%D0%BE%D1%80%D1%8C_%D0%A1%D0%B0%D1%80%D1%83%D1%85%D0%B0%D0%BD%D0%BE%D0%B2&quot; class=&quot;mw-redirect&quot; title=&quot;Игорь Саруханов&quot;&gt;Игорь Саруханов&lt;/a&gt; / &lt;a href=&quot;/wiki/%D0%A1%D0%B8%D0%BC%D0%BE%D0%BD_%D0%9E%D1%81%D0%B8%D0%B0%D1%88%D0%B2%D0%B8%D0%BB%D0%B8&quot; class=&quot;mw-redirect&quot; title=&quot;Симон Осиашвили&quot;&gt;Симон Осиашвили&lt;/a&gt;)&lt;/sub&gt;
