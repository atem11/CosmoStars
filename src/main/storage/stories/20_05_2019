			&lt;div class=&quot;thumb&quot; style=&quot;width: 750px;&quot;&gt;&lt;div style=&quot;margin:0px auto;&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:Volodymyr_Zelensky_2019_presidential_inauguration_19.jpg&quot; class=&quot;image&quot;&gt;&lt;img alt=&quot;Volodymyr Zelensky 2019 presidential inauguration 19.jpg&quot; src=&quot;//upload.wikimedia.org/wikipedia/commons/5/53/Volodymyr_Zelensky_2019_presidential_inauguration_19.jpg&quot; decoding=&quot;async&quot; width=&quot;750&quot; height=&quot;500&quot; data-file-width=&quot;1042&quot; data-file-height=&quot;695&quot; /&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;
		&lt;/div&gt;&lt;/li&gt;
		&lt;li class=&quot;gallerybox&quot; style=&quot;width: 752.66666666667px&quot;&gt;&lt;div style=&quot;width: 752.66666666667px&quot;&gt;
			&lt;div class=&quot;thumb&quot; style=&quot;width: 750.66666666667px;&quot;&gt;&lt;div style=&quot;margin:0px auto;&quot;&gt;&lt;a href=&quot;/wiki/%D0%A4%D0%B0%D0%B9%D0%BB:Volodymyr_Zelensky_2019_presidential_inauguration_20.jpg&quot; class=&quot;image&quot;&gt;&lt;img alt=&quot;Volodymyr Zelensky 2019 presidential inauguration 20.jpg&quot; src=&quot;//upload.wikimedia.org/wikipedia/commons/f/fd/Volodymyr_Zelensky_2019_presidential_inauguration_20.jpg&quot; decoding=&quot;async&quot; width=&quot;751&quot; height=&quot;500&quot; data-file-width=&quot;1043&quot; data-file-height=&quot;695&quot; /&gt;&lt;/a&gt;&lt;/div&gt;&lt;/div&gt;
		&lt;/div&gt;&lt;/li&gt;
		&lt;li class=&quot;gallerybox&quot; style=&quot;width: 752.66666666667px&quot;&gt;&lt;div style=&quot;width: 752.66666666667px&quot;&gt;
