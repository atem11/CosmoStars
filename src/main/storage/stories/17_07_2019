&lt;h2&gt;&lt;span id=&quot;.D0.A1.D0.BC._.D1.82.D0.B0.D0.BA.D0.B6.D0.B5&quot;&gt;&lt;/span&gt;&lt;span class=&quot;mw-headline&quot; id=&quot;См._также&quot;&gt;См. также&lt;/span&gt;&lt;span class=&quot;mw-editsection&quot;&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;[&lt;/span&gt;&lt;a href=&quot;/w/index.php?title=%D0%A0%D0%B0%D0%B7%D0%B3%D0%BE%D0%BD_%D0%B8%D0%B7%D0%B1%D0%B8%D1%80%D0%B0%D1%82%D0%B5%D0%BB%D0%B5%D0%B9_%D1%83_%D0%9C%D0%BE%D1%81%D0%B3%D0%BE%D1%80%D0%B8%D0%B7%D0%B1%D0%B8%D1%80%D0%BA%D0%BE%D0%BC%D0%B0:_%D0%91%D0%B8%D0%BB%D0%B8,_%D1%87%D1%82%D0%BE%D0%B1%D1%8B_%D0%BD%D0%B0%D1%80%D0%BE%D0%B4_%D1%80%D0%B0%D0%B7%D0%BE%D1%88%D0%B5%D0%BB%D1%81%D1%8F&amp;amp;action=edit&amp;amp;section=1&quot; title=&quot;Редактировать раздел «См. также»&quot;&gt;править&lt;/a&gt;&lt;span class=&quot;mw-editsection-bracket&quot;&gt;]&lt;/span&gt;&lt;/span&gt;&lt;/h2&gt;
