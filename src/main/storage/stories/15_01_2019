&lt;td&gt;&lt;a href=&quot;/wiki/%D0%A8%D0%B2%D0%B5%D1%86%D0%B8%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Швеция&quot;&gt;Швеция&lt;/a&gt;
&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;
&lt;/center&gt;
&lt;p&gt;&lt;a href=&quot;/wiki/17_%D0%BD%D0%BE%D1%8F%D0%B1%D1%80%D1%8F_2018_%D0%B3%D0%BE%D0%B4%D0%B0&quot; class=&quot;mw-redirect&quot; title=&quot;17 ноября 2018 года&quot;&gt;17 ноября 2018 года&lt;/a&gt; у &lt;a href=&quot;/wiki/%D0%A0%D1%83%D1%81%D1%81%D0%BA%D0%B8%D0%B5_%D0%92%D0%B8%D0%BA%D0%B8%D0%BD%D0%BE%D0%B2%D0%BE%D1%81%D1%82%D0%B8&quot; class=&quot;mw-redirect&quot; title=&quot;Русские Викиновости&quot;&gt;Русских Викиновостей&lt;/a&gt; также &lt;a href=&quot;/wiki/%D0%A0%D1%83%D1%81%D1%81%D0%BA%D0%B8%D0%B5_%D0%92%D0%B8%D0%BA%D0%B8%D0%BD%D0%BE%D0%B2%D0%BE%D1%81%D1%82%D0%B8_%D0%B7%D0%B0%D0%B2%D0%B5%D0%BB%D0%B8_Instagram&quot; title=&quot;Русские Викиновости завели Instagram&quot;&gt;появился&lt;/a&gt; аккаунт в Instagram: &lt;a rel=&quot;nofollow&quot; class=&quot;external text&quot; href=&quot;https://www.instagram.com/russianwikinews/&quot;&gt;@russianwikinews&lt;/a&gt;.
&lt;/p&gt;&lt;p&gt;&lt;br /&gt;
&lt;/p&gt;
&lt;table class=&quot;metadata plainlinks ambox ambox-notice&quot;&gt;
&lt;tbody&gt;&lt;tr&gt;
