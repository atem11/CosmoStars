&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%A0%D0%BE%D0%B4%D0%B8%D0%B2%D1%88%D0%B8%D0%B5%D1%81%D1%8F_15_%D1%8F%D0%BD%D0%B2%D0%B0%D1%80%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Родившиеся 15 января&quot;&gt;Родившиеся 15 января&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;
