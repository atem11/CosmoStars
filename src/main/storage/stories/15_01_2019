&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%9F%D1%80%D0%B0%D0%B7%D0%B4%D0%BD%D0%B8%D0%BA%D0%B8_15_%D1%8F%D0%BD%D0%B2%D0%B0%D1%80%D1%8F&quot; class=&quot;mw-redirect&quot; title=&quot;Праздники 15 января&quot;&gt;Праздники 15 января&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;&lt;/ul&gt;
