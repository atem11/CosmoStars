&lt;li&gt;&lt;b&gt;&lt;a href=&quot;/wiki/%D0%9B%D0%B5%D0%BD%D1%82%D0%B0_%D0%BD%D0%BE%D0%B2%D0%BE%D1%81%D1%82%D0%B5%D0%B9_3_%D0%B0%D0%BF%D1%80%D0%B5%D0%BB%D1%8F_2018_%D0%B3%D0%BE%D0%B4%D0%B0&quot; title=&quot;Лента новостей 3 апреля 2018 года&quot;&gt;Лента новостей 3 апреля 2018 года&lt;/a&gt;&lt;/b&gt;&lt;/li&gt;&lt;/ul&gt;
&lt;/td&gt;
&lt;td class=&quot;widthhack&quot;&gt;
&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;
