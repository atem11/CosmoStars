&lt;li&gt;&lt;a href=&quot;/wiki/%D0%9B%D0%B5%D0%BD%D1%82%D0%B0_%D0%BD%D0%BE%D0%B2%D0%BE%D1%81%D1%82%D0%B5%D0%B9_9_%D0%B8%D1%8E%D0%BB%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0&quot; title=&quot;Лента новостей 9 июля 2019 года&quot;&gt;Лента новостей 9 июля 2019 года&lt;/a&gt;&lt;/li&gt; 
&lt;li&gt;&lt;a href=&quot;/wiki/%D0%9E%D0%B6%D0%B8%D0%B4%D0%B0%D0%B5%D0%BC%D1%8B%D0%B5_%D1%81%D0%BE%D0%B1%D1%8B%D1%82%D0%B8%D1%8F_9_%D0%B8%D1%8E%D0%BB%D1%8F_2019_%D0%B3%D0%BE%D0%B4%D0%B0&quot; title=&quot;Ожидаемые события 9 июля 2019 года&quot;&gt;Ожидаемые события 9 июля 2019 года&lt;/a&gt;&lt;/li&gt;&lt;/ul&gt;
