&lt;div style=&quot;text-decoration:none; background:#cfe3ff; padding:2px; text-align:center; font-size:125%; margin:0.4em;&quot;&gt;&lt;a href=&quot;/wiki/%D0%9A%D0%B0%D1%82%D0%B5%D0%B3%D0%BE%D1%80%D0%B8%D1%8F:%D0%98%D0%B7%D0%B2%D0%B8%D0%BD%D0%B5%D0%BD%D0%B8%D1%8F&quot; title=&quot;Категория:Извинения&quot;&gt;Извинения&lt;/a&gt;&lt;/div&gt;&lt;ul&gt;
